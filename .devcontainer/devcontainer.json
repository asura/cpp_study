{
	"name": "GCC 10",
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"extensions": [
		"ms-vscode.cpptools",
		"twxs.cmake",
		"cschlosser.doxdocgen",
		"donjayamanne.githistory"
	]
}